@using InteractHealthProDatabase.Models.Enums;
@model ClientDetailsViewModel;

@{
    bool isEdit = Model.Id != 0;
    ViewData["Title"] = "Details";
}

<h1> Client Profile</h1>

<div>
    <h2> Of @Html.DisplayFor(model => model.ContactName)</h2>


    @*===================================== NAVIGATION CONVENIENCE ===============================================================*@

    <!-- Add this in the view where you want to trigger the download -->
    <!-- Navigation Table Field -->
    <div class="col-md-10">
        <div class="card">
            <div class="card-header">
                Navigation
            </div>
            <div class="card-body" id="navigationCard">

                <ul>
                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            <a asp-action="CreateOrEdit" asp-route-id="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Basic Info</a>
                        </div>
                    </li>
                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            <a asp-controller="AccidentDetail" asp-action="CreateOrEdit" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Accident</a>
                        </div>
                    </li>
                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            <a asp-controller="Case" asp-action="CreateOrEdit" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Lawyer</a>
                        </div>
                    </li>
                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            @if (Model.WorkHistory == null)
                            {
                                <a asp-controller="WorkHistory" asp-action="Create" asp-route-clientId="@Model.Id" class="btn btn-group btn-primary col-auto mx-2">Work History</a>
                            }
                            else
                            {
                                <a asp-controller="WorkHistory" asp-action="Edit" asp-route-id="@Model.WorkHistory.Id" class="btn btn-group btn-primary col-auto mx-2">Work History</a>
                            }
                        </div>
                    </li>
                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            <a asp-controller="HealthFile" asp-action="CreateOrEdit" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Health File</a>
                        </div>
                    </li>
                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            <a asp-controller="MedicalHistoryAccident" asp-action="CreateOrEdit" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2 pe-0">Medical History</a>
                        </div>
                    </li>
                </ul>


                <ul>

                    <li>
                        <div class="btn-group fixed-width-button" role="group">
                            <a asp-controller="BodyPart" asp-action="CreateOrEdit" asp-route-id="@Model?.BodyPart?.Id" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Injuries</a>
                        </div>
                    </li>


                    <li>
                        @if (@Model?.Concussion == null)
                        {
                            <div class="btn-group fixed-width-button" role="group">
                                <a asp-controller="Concussion" asp-action="CreateOrEdit" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Concussion</a>
                            </div>
                        }
                        else
