                        <div class="dropdown-content">
                            @Html.ActionLink("Download OCF-1 PDF", "DownloadOCF1", new { id = Model.Id })
                            @Html.ActionLink("Download OCF-2 PDF", "DownloadOCF2", new { id = Model.Id })
                            @Html.ActionLink("Download OCF-3 PDF", "DownloadOCF3", new { id = Model.Id })
                            @Html.ActionLink("Download OCF-6 PDF", "DownloadOCF6", new { id = Model.Id })
                            @Html.ActionLink("Download OCF-10 PDF", "DownloadOCF10", new { id = Model.Id })
                            <!-- Add more links as needed -->
                        </div>

