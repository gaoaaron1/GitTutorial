                            <div class="btn-group fixed-width-button" role="group">
                                <a asp-controller="Concussion" asp-action="CreateOrEdit" asp-route-id="@Model?.Concussion.Id" asp-route-clientId="@Model?.Id" class="btn btn-group btn-primary col-auto mx-2">Concussion</a>
                            </div>
                        }
                    </li>
                    <li>
